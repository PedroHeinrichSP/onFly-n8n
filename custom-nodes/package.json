{
  "name": "n8n-nodes-onfly",
  "version": "0.1.0",
  "description": "Custom nodes para projeto onFly n8n",
  "keywords": [
    "n8n-community-node-package",
    "n8n-nodes",
    "onfly"
  ],
  "license": "MIT",
  "homepage": "https://n8n.io",
  "author": {
    "name": "onFly Team",
    "email": "dev@onfly.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PedroHeinrichSP/onFly-n8n.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
    ],
    "nodes": [
      "dist/nodes/OnFlyNode/OnFlyNode.node.js",
      "dist/nodes/Random/Random.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}